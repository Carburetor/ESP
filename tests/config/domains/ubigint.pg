CREATE OR REPLACE FUNCTION
  "test ubigint can't be written in messages if input < 0"()
RETURNS SETOF TEXT
AS $$
BEGIN
    PREPARE write_message AS INSERT INTO "messages"
      (
        "id",
        "stream_name",
        "position",
        "type",
        "data",
        "metadata"
      )
    VALUES
      (
        NULL,
        'account-123',
        (-1),
        '',
        '{}'::jsonb,
        '{}'::jsonb
      );

  return query SELECT throws_imatching(
    'write_message',
    '.*ubigint\sviolates\scheck\sconstraint.*'
  );
END;
$$ LANGUAGE plpgsql;
